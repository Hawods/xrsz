<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hawods.mapper.SeoMapper">
    <resultMap id="seoResultMap" type="org.hawods.entity.Seo">
        <id property="id" column="id" />
        <result property="type" column="type" typeHandler="org.apache.ibatis.type.EnumOrdinalTypeHandler"/>
    </resultMap>

    <select id="selectSeo" resultMap="seoResultMap">
        select * from ss_seo where id = #{id}
    </select>
</mapper>
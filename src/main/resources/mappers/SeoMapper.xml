<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.hawods.mapper.SeoMapper">
    <select id="selectSeo" resultType="seo">
        select * from ss_seo where id = #{id}
    </select>

    <select id="selectByType" resultType="seo">
        select * from ss_seo where type = #{type}
    </select>
</mapper>